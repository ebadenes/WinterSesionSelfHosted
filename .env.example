BASE_DOMAIN=example.com
LE_EMAIL=admin@example.com
TZ=UTC
DATA_DIR=/srv/data

# Traefik
TRAEFIK_DASHBOARD_USERS=admin:REPLACE_WITH_HTPASSWD_HASH

# Immich
IMMICH_VERSION=release
IMMICH_DB_USERNAME=immich
IMMICH_DB_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
IMMICH_DB_DATABASE_NAME=immich

# n8n
N8N_ENCRYPTION_KEY=REPLACE_WITH_RANDOM_KEY
N8N_BASIC_AUTH_ACTIVE=false
N8N_BASIC_AUTH_USER=
N8N_BASIC_AUTH_PASSWORD=

# Docmost
DOCMOST_APP_SECRET=REPLACE_WITH_32_CHARS_MIN
DOCMOST_DB_PASSWORD=REPLACE_WITH_STRONG_PASSWORD

# Vaultwarden
VW_SIGNUPS_ALLOWED=false
VW_ADMIN_TOKEN=REPLACE_WITH_ADMIN_TOKEN_OR_LEAVE_EMPTY

# Zulip
ZULIP_ADMIN_EMAIL=admin@example.com
ZULIP_POSTGRES_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
ZULIP_RABBITMQ_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
ZULIP_MEMCACHED_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
ZULIP_REDIS_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
ZULIP_SECRET_KEY=REPLACE_WITH_LONG_RANDOM_SECRET
ZULIP_SSL_CERTIFICATE_GENERATION=self-signed

# Linkwarden
LINKWARDEN_POSTGRES_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
LINKWARDEN_NEXTAUTH_SECRET=REPLACE_WITH_SECRET
LINKWARDEN_MEILI_MASTER_KEY=REPLACE_WITH_SECRET

# Paperless-ngx
PAPERLESS_DB_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
PAPERLESS_SECRET_KEY=REPLACE_WITH_LONG_RANDOM_SECRET

# Planka
PLANKA_DB_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
PLANKA_SECRET_KEY=REPLACE_WITH_LONG_RANDOM_SECRET

# Nextcloud
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
NEXTCLOUD_DB_PASSWORD=REPLACE_WITH_STRONG_PASSWORD
